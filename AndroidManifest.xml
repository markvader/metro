<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.braveo.sydney.transport"
      android:versionCode="16" android:versionName="1.0.0">

        <uses-sdk
                android:minSdkVersion="2"
        />

    <application android:label="@string/app_name" android:icon="@drawable/sydtrans2">
        <activity android:name=".SydneyTransport"
                  android:label="@string/app_name" android:configChanges="keyboardHidden|orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
    <activity android:name=".StationView" android:configChanges="keyboardHidden|orientation"></activity>
<activity android:name=".LineDetailsView" android:configChanges="keyboardHidden|orientation"></activity>
<activity android:name="SydneyTransHistoryView"></activity>
<activity android:name="DownloadListView"></activity>
<activity android:name="AboutView"></activity>
<activity android:name="HelpView"></activity>
</application>

<uses-permission android:name="android.permission.INTERNET"></uses-permission>

        
</manifest> 